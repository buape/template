{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Bot",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "moduleResolution": "node",
        "lib": ["ESNext"],
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitOverride": true,
        "pretty": true,
        "baseUrl": "./"
    }
}
